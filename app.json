{
  "expo": {
    "name": "CleanShot",
    "slug": "cleanshot-app",
    "version": "1.0.0",
    "owner": "aviataravindhdotme",
    "orientation": "portrait",
    "cli": {
      "appVersionSource": "remote"
    },
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#10B981"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aravindh.cleanshot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#10B981"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aravindh.cleanshot"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "b9836b05-2511-4003-8d9e-802cb38d36b9"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b9836b05-2511-4003-8d9e-802cb38d36b9"
    }
  }
}
